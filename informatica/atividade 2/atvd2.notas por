programa
{
	inclua biblioteca Matematica --> mat
	funcao inicio()
	{
	  inteiro primeiranota
	  inteiro segundanota
	  real soma

      //coleta de dados
      
      escreva("digite sua primera nota: ")
      leia (primeiranota)
         
      escreva("digite sua segunda nota:")
      leia(segundanota)

     //processamento de dados

    	soma = primeiranota + segundanota

    	
    //saida de dados 

	








    

	  
	}
}
