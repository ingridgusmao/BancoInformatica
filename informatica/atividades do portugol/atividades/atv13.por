programa
{
	funcao inicio()
	{
	real cel
	real fah
	
	escreva("Insira a temperatura em valores de Celsius: ")
	leia(cel)
	
	fah = (cel * 9 / 5) + 32

	escreva("A temperatura em valors de Fahrenheit é de: " + fah)
	}
}
